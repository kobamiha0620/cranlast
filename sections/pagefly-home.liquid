    // スクロール処理は重いので画像が表示されている間だけにする
    if (scroll_top_px < main_visual_scroll_height_px) {
      var scroll_ratio = scroll_top_px / main_visual_scroll_height_px;
      var image_height = main_visual_height_vw + ((main_visual_height_vw * 0.05) * scroll_ratio); // 数字部分が拡大率
      document.querySelector(".pc_main_visual_picture").style.height = image_height + "vw";
    }
    if (margin_before_sign_scroll_height_px < scroll_bottom_px) {
      var scroll_px = scroll_bottom_px - margin_before_sign_scroll_height_px;
      var scroll_ratio = scroll_px / sign_scroll_height_px;
      var image_height = sign_picture_height_px + ((sign_picture_height_px * 0.08) * scroll_ratio); // 数字部分が拡大率
